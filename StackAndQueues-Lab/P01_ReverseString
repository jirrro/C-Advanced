using System;
using System.Collections.Generic;


namespace P01_ReverseString
{
    class Program
    {
        static void Main(string[] args)
        {
            string text = Console.ReadLine();
            Stack<char> reverse = new Stack<char>(text);
            foreach (var item in reverse)
            {
                Console.Write(item);
            }
            Console.WriteLine();
        }
    }
}
